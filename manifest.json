{
  "manifest_version": 2,
 
  "name": "GrandmaFB",
  "version": "0.1.0",
  "description": "Lateral blog post demo extension",

  "browser_action": {
    "default_icon": "grandma.png",
	"default_title": "Crazy"
  },
 
  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["jquery-2.2.5.min.js","content.js"],
    "matches": ["<all_urls>"]
  }],

  "background": {
  "scripts": ["background.js"]
  }

 
}